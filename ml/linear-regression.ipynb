    "from sklearn.linear_model import LinearRegression, LogisticRegression\n",
    "\n",
    "X = [[1], [2], [3], [4], [5]]\n",
    "y = [40, 50, 60, 70, 80]\n",
    "\n",
    "model = LinearRegression()\n",
    "model.fit(X, y)\n",
    "hours = float(input(\"Enter Study hours\"))\n",
    "predicted_marks = model.predict([[hours]])\n",
    "print(predicted_marks)"
   ]
  }
